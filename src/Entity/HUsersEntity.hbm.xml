<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.HUsersEntity" table="h_users" schema="aaa">
        <id name="haidaxueSn">
            <column name="haidaxue_sn" sql-type="varchar(15)" length="15"/>
        </id>
        <property name="userName">
            <column name="user_name" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="schoolName">
            <column name="school_name" sql-type="varchar(30)" length="30" not-null="false"/>
        </property>
        <property name="majorName">
            <column name="major_name" sql-type="varchar(30)" length="30" not-null="false"/>
        </property>
        <property name="departmentalName">
            <column name="departmental_name" sql-type="varchar(30)" length="30" not-null="false"/>
        </property>
        <property name="userAge">
            <column name="user_age" sql-type="int(4)" not-null="false"/>
        </property>
        <property name="currentLocation">
            <column name="current_location" sql-type="varchar(30)" length="30" not-null="false"/>
        </property>
        <property name="emailAddress">
            <column name="email_address" sql-type="varchar(40)" length="40" not-null="false"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(16)" length="16" not-null="true"/>
        </property>
        <property name="nickname">
            <column name="nickname" sql-type="varchar(16)" length="16" not-null="false"/>
        </property>
        <property name="telephoneNumber">
            <column name="telephone_number" sql-type="varchar(15)" length="15" not-null="false"/>
        </property>
        <property name="id">
            <column name="ID" sql-type="varchar(10)" length="10" not-null="false"/>
        </property>
        <property name="userLabel">
            <column name="user_label" sql-type="varchar(3)" length="3" not-null="false"/>
        </property>
        <property name="originSchool">
            <column name="origin_school" sql-type="varchar(30)" length="30" not-null="false"/>
        </property>
    </class>

</hibernate-mapping>